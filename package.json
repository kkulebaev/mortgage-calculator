{
  "name": "mortgage-calculator",
  "version": "1.0.421",
  "type": "module",
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run eslint && pnpm run stylelint && pnpm run compile && pnpm run test",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "eslint:inspect-config": "eslint ./src --inspect-config",
    "compile": "vue-tsc",
    "fmt": "prettier --write ./src",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "pull-translations": "tolgee pull",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.6.0",
    "@fortawesome/free-brands-svg-icons": "6.6.0",
    "@fortawesome/free-regular-svg-icons": "6.6.0",
    "@fortawesome/free-solid-svg-icons": "6.6.0",
    "@fortawesome/vue-fontawesome": "3.0.6",
    "@vueuse/core": "11.1.0",
    "animate.css": "4.1.1",
    "element-plus": "2.8.0",
    "pinia": "2.2.0",
    "valibot": "^0.42.0",
    "vue": "3.5.0",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.0"
  },
  "devDependencies": {
    "@kkulebaev/prettier-config": "0.2.0",
    "@tolgee/cli": "2.3.0",
    "@types/node": "20.16.0",
    "@vitejs/plugin-vue": "5.1.0",
    "@vitest/ui": "2.1.0",
    "@vue/test-utils": "2.4.5",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-vue": "9.29.0",
    "jsdom": "25.0.0",
    "nano-staged": "0.8.0",
    "postcss": "8.4.38",
    "postcss-custom-media": "11.0.0",
    "postcss-html": "1.7.0",
    "prettier": "3.3.0",
    "simple-git-hooks": "2.11.1",
    "stylelint": "16.10.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "5.4.0",
    "vite-plugin-sitemap": "0.7.1",
    "vitest": "2.1.0",
    "vue-tsc": "2.1.0"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  },
  "packageManager": "pnpm@9.12.0"
}
