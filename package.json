{
  "name": "package.json",
  "private": true,
  "version": "1.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "lint:types": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@vueuse/core": "9.5.0",
    "animate.css": "4.1.1",
    "element-plus": "2.2.21",
    "pinia": "2.0.24",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/ui": "0.25.2",
    "@vue/test-utils": "2.2.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unicorn": "44.0.2",
    "eslint-plugin-vue": "9.7.0",
    "jsdom": "20.0.2",
    "nano-staged": "0.8.0",
    "sass": "1.56.1",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.8.4",
    "vite": "3.2.4",
    "vitest": "0.25.2",
    "vue-tsc": "1.0.9"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  }
}
