{
  "name": "package.json",
  "private": true,
  "version": "1.0.242",
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run eslint && pnpm run stylelint && pnpm run compile && pnpm run test",
    "eslint": "eslint --ext .js,.ts,.vue src",
    "eslint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "compile": "vue-tsc",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.0",
    "@fortawesome/free-brands-svg-icons": "6.5.0",
    "@fortawesome/free-regular-svg-icons": "6.5.0",
    "@fortawesome/free-solid-svg-icons": "6.5.0",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "@vueuse/core": "10.6.1",
    "animate.css": "4.1.1",
    "element-plus": "2.4.3",
    "pinia": "2.1.7",
    "vue": "3.3.9",
    "vue-i18n": "9",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-vue": "4.5.0",
    "@vitest/ui": "0.34.6",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vue": "9.19.2",
    "jsdom": "23.0.1",
    "nano-staged": "0.8.0",
    "postcss": "8.4.31",
    "postcss-custom-media": "10.0.2",
    "postcss-html": "1.5.0",
    "prettier": "3.1.0",
    "simple-git-hooks": "2.9.0",
    "stylelint": "15.11.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-plugin-sitemap": "0.5.3",
    "vitest": "0.34.6",
    "vue-tsc": "1.8.24"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  }
}
