{
  "name": "mortgage-calculator",
  "version": "1.0.421",
  "type": "module",
  "engines": {
    "node": ">=16.14",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run eslint && pnpm run stylelint && pnpm run compile && pnpm run test",
    "eslint": "eslint --ext .js,.ts,.vue src",
    "eslint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "compile": "vue-tsc",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-brands-svg-icons": "6.5.2",
    "@fortawesome/free-regular-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/vue-fontawesome": "3.0.6",
    "@vueuse/core": "10.9.0",
    "animate.css": "4.1.1",
    "element-plus": "2.7.0",
    "pinia": "2.1.7",
    "vue": "3.4.22",
    "vue-i18n": "9",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/ui": "1.5.0",
    "@vue/test-utils": "2.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-vue": "9.25.0",
    "jsdom": "24.0.0",
    "nano-staged": "0.8.0",
    "postcss": "8.4.38",
    "postcss-custom-media": "10.0.4",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "simple-git-hooks": "2.11.1",
    "stylelint": "16.3.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.4.5",
    "vite": "5.2.9",
    "vite-plugin-sitemap": "0.5.3",
    "vitest": "1.5.0",
    "vue-tsc": "2.0.13"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  }
}
