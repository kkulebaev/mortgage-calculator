{
  "name": "package.json",
  "private": true,
  "version": "1.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "lint:types": "vue-tsc",
    "lint:style": "stylelint \"**/*.{css,vue}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/vue-fontawesome": "3.0.2",
    "@vueuse/core": "9.6.0",
    "animate.css": "4.1.1",
    "element-plus": "2.2.26",
    "pinia": "2.0.28",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@gitlab/stylelint-config": "4.1.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitest/ui": "0.25.6",
    "@vue/test-utils": "2.2.6",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-vue": "9.8.0",
    "jsdom": "20.0.3",
    "nano-staged": "0.8.0",
    "postcss": "8.4.20",
    "postcss-custom-media": "9.0.1",
    "postcss-html": "1.5.0",
    "simple-git-hooks": "2.8.1",
    "stylelint": "14.16.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "typescript": "4.9.4",
    "vite": "4.0.1",
    "vite-plugin-sitemap": "0.3.0",
    "vitest": "0.25.6",
    "vue-tsc": "1.0.13"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  }
}
