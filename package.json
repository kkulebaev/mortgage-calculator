{
  "name": "package.json",
  "private": true,
  "version": "1.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run eslint && pnpm run stylelint && pnpm run lint:types && pnpm run test",
    "eslint": "eslint --ext .js,.ts,.vue src",
    "eslint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "lint:types": "vue-tsc",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/vue-fontawesome": "3.0.3",
    "@vueuse/core": "10.0.2",
    "animate.css": "4.1.1",
    "element-plus": "2.3.3",
    "pinia": "2.0.34",
    "vue": "3.2.47",
    "vue-i18n": "9",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitejs/plugin-vue": "4.1.0",
    "@vitest/ui": "0.30.1",
    "@vue/test-utils": "2.3.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unicorn": "46.0.0",
    "eslint-plugin-vue": "9.11.0",
    "jsdom": "21.1.1",
    "nano-staged": "0.8.0",
    "postcss": "8.4.23",
    "postcss-custom-media": "9.1.3",
    "postcss-html": "1.5.0",
    "prettier": "2.8.7",
    "simple-git-hooks": "2.8.1",
    "stylelint": "15.5.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "33.0.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vite-plugin-sitemap": "0.3.0",
    "vitest": "0.30.1",
    "vue-tsc": "1.2.0"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  }
}
