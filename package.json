{
  "name": "mortgage-calculator",
  "version": "1.0.421",
  "type": "module",
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run eslint && pnpm run stylelint && pnpm run compile && pnpm run test",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "eslint:inspect-config": "eslint ./src --inspect-config",
    "compile": "vue-tsc",
    "fmt": "prettier --write ./src",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,vue}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "pull-translations": "tolgee pull",
    "print-config:stylelint": "stylelint \"./src/app.vue\" --print-config"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/vue-fontawesome": "3.1.0",
    "@vueuse/core": "14.1.0",
    "animate.css": "4.1.1",
    "element-plus": "2.12.0",
    "pinia": "3.0.1",
    "valibot": "^1.2.0",
    "vue": "3.5.0",
    "vue-i18n": "11.2.1",
    "vue-router": "4.6.0"
  },
  "devDependencies": {
    "@kkulebaev/prettier-config": "0.2.0",
    "@tolgee/cli": "2.14.0",
    "@types/node": "24.10.0",
    "@vitejs/plugin-vue": "6.0.0",
    "@vitest/ui": "4.0.1",
    "@vue/test-utils": "2.4.5",
    "eslint": "9.39.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-plugin-vue": "10.6.0",
    "jsdom": "27.3.0",
    "nano-staged": "0.9.0",
    "postcss": "8.5.0",
    "postcss-custom-media": "11.0.0",
    "postcss-html": "1.8.0",
    "prettier": "3.7.1",
    "simple-git-hooks": "2.13.0",
    "stylelint": "16.26.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.49.0",
    "vite": "7.2.0",
    "vite-plugin-sitemap": "0.8.0",
    "vitest": "4.0.1",
    "vue-tsc": "3.1.0"
  },
  "nano-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/simple-git-hooks && ./node_modules/.bin/nano-staged"
  },
  "packageManager": "pnpm@9.15.0"
}
